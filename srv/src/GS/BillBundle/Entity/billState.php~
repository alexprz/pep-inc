<?php

namespace GS\BillBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * billState
 *
 * @ORM\Table(name="gs_bill_state")
 * @ORM\Entity(repositoryClass="GS\BillBundle\Repository\billStateRepository")
 */
class BillState
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="name", type="string", length=255)
     */
    private $name;


    /**
     * Get id.
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name.
     *
     * @param string $name
     *
     * @return billState
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name.
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
